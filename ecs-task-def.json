{
  "family": "hello-world-task",
  "containerDefinitions": [
    {
      "name": "hello-world",
      "image": "637423558639.dkr.ecr.ap-south-1.amazonaws.com/hello-world:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
